# managed by chef, changes will be overwritten
check <%= @type.to_s %> <%= @name %>
  <% if @withs %>
    <%= @withs %>
  <% end %>
  <% if @alert %><%= @alert %><% end -%>
  <% if @start %>
  start program = "<%= @start %>"
  <% if @start_as %>
    as <%= @start_as %>
  <% end %>
  <% end -%>
  <% if @stop %>
  stop program = "<%= @stop %>"
  <% if @stop_as %>
    as <%= @stop_as %>
  <% end %>
  <% end -%>
  <% if @mode %>
  mode <%= @mode.to_s %>
  <% end %>
  <% if @group -%>
  <% Array(@group).each do |g| -%>
  group <%= g %>
  <% end -%>
  <% end -%>
  <% if @rule -%>
  <% Array(@rule).each do |r| -%>
  <%= r %>
  <% end -%>
  <% end -%>
  <% if @depends -%>
  <% Array(@depends).each do |d| -%>
  depends <%= d %>
  <% end -%>
  <% end -%>
